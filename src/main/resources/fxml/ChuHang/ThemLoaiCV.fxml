<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tiendat.demo.Controller.ControllerChuHang.ControllerThemLoaiCVChuyenHang">
   <children>
      <Label text="Thêm Loại CV Chuyến Hàng" />
      <Label fx:id="id_LabelChuyenHang" text="Chuyến Hàng" />
      <HBox spacing="10.0">
         <children>
            <Label prefWidth="50.0" text="Loại CV" />
            <JFXComboBox fx:id="id_ComboboxLoaiCV" prefWidth="300.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <JFXButton onAction="#Them" text="Thêm" />
      <Label text="Bảng Loại CV" />
      <TableView fx:id="id_TableLoaiCV">
        <columns>
          <TableColumn fx:id="id_CotTen" prefWidth="75.0" text="Tên" />
          <TableColumn fx:id="id_CotNguyenChuyen" prefWidth="107.0" text="Nguyên Chuyến" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
