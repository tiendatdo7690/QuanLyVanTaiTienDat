<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tiendat.demo.Controller.ControllerHang.ThemLoaiChiHoController">
   <children>
      <Label text="Thêm Loại Chi Hộ" />
      <HBox spacing="10.0">
         <children>
            <Label text="Tên Loại Chi Hộ" />
            <JFXTextField fx:id="id_TFTen" prefWidth="300.0" />
         </children>
      </HBox>
      <JFXButton onAction="#ThemLoaiChiHo" text="Thêm" />
      <TableView fx:id="id_TableLoaiChiHo" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="id_ColTenLoaiChiHo" prefWidth="75.0" text="Tên" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
